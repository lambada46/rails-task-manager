<h1>edit task</h1>

<%= form_for @task do |f| %>
<%=f.label :title  %>
<%=f.text_field :title %>
<%=f.label :description %>
<%=f.text_field :description%>
<%=f.label :location %>
<%=f.text_field :location%>
<%=f.label :when %>
<%=f.text_field :when%>
<%=f.label :status %>
<%=f.text_field :status%>

<%=f.submit%>
<%end%>
